<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
        "http://www.springframework.org/dtd/spring-beans-2.0.dtd">


<beans>

	<bean id="cou" class="com.model.Course">
		<property name="cid" value="102"></property>
		<property name="cname" value="java"></property>

	</bean>

	<bean id="fac" class="com.model.Faculty">
		<property name="fid" value="200"></property>
		<property name="fname" value="NilimaMam"></property>
		<property name="course" ref="cou"></property>
	</bean>
	
	<bean id="batch" class="com.model.Batch">
	<property name="bid" value="300"></property>
	<property name="bname" value="JB53"></property>
	<property name="faculty" ref="fac"></property>
	
   </bean>

    <bean id="stu" class="com.model.Student">
    <property name="stuid" value="209"></property>
    <property name="stuname" value="pooja"></property>
    <property name="batch" ref="batch"></property>
    
    
    
    </bean>




</beans>






